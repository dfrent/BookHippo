<div class="search-wrapper">


<% if @google_items %>

  <% if @user.any? %>
    <%= render partial: "user_search", locals: {user: @user} %>
  <% end %>

  <% if @books.any? %>
    <%= render partial: "book_search", locals: {books: @books} %>
  <% end %>
<% else %>
  <%= render partial: "no_results"%>
<% end %>

</div>
